<Window x:Class="TeamBuilderPkmn.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TeamBuilderPkmn"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="55*"/>
            <RowDefinition Height="400*"/>
        </Grid.RowDefinitions>
        <!--<ComboBox ItemsSource="{Binding cbItems}" SelectedItem="{Binding SelectedcbItem}"/><-->
        <ComboBox x:Name="typeOne"  HorizontalAlignment="Left" Height="45" Margin="45,0,0,0" Grid.Row="1" VerticalAlignment="Top" Width="230" SelectedIndex="0"></ComboBox>
        <ComboBox x:Name="typeTwo"  HorizontalAlignment="Right" Height="45" Margin="0,0,45,0" Grid.Row="1" VerticalAlignment="Top" Width="230" SelectedIndex="0"></ComboBox>
    </Grid>
</Window>
